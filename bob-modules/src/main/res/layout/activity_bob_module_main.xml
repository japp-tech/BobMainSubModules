<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BobModuleMainActivity">

    <Button
        android:id="@+id/btnGo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to Hl Main Screen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnGoPlMl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to Pl Ml Main Screen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnGo"
        android:layout_marginTop="10dp"/>

    <Button
        android:id="@+id/btnGoEL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to EL Main Screen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnGoPlMl"
        android:layout_marginTop="10dp"/>

    <Button
        android:id="@+id/btnGoAL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to AL Main Screen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnGoEL"
        android:layout_marginTop="10dp"/>



</androidx.constraintlayout.widget.ConstraintLayout>